local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Survive The Night", "DarkTheme")
local Speed = Window:NewTab("Speed")
local Speed = Speed:NewSection("Speed")
local Player = game.Players.Testerextra

Player.Chatted:connect(function(cht)
    if cht:match("/e slow") then
        
    end 
end)
        
Speed:NewKeybind("KeybindText", "KeybindInfo", Enum.KeyCode.F, function()	Library:ToggleUI()end)
Speed:NewToggle("Speed", "ToggleInfo", function(state)
    if state then
        _G.Toggle = true

while _G.Toggle do
   local gmt = getrawmetatable(game)
local oldIndex = gmt.__index
setreadonly(gmt, false)

gmt.__index = newcclosure(function(Self,Key)
    if tostring(Self) == "Humanoid" and tostring(Key) == "WalkSpeed" then
        return
    end
    return oldIndex(Self,Key)
end)

while wait() do
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 20
end
end
    else
        print("Toggle Off")    
    end
end)
